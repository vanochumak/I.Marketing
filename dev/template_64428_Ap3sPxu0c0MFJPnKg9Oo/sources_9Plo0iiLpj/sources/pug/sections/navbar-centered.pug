.rd-navbar-wrap.rd-navbar-centered
  nav.rd-navbar(data-layout='rd-navbar-fixed', data-sm-layout='rd-navbar-fixed', data-md-layout='rd-navbar-fullwidth', data-md-device-layout='rd-navbar-fixed', data-lg-layout='rd-navbar-static', data-lg-device-layout="rd-navbar-static",  data-md-stick-up-offset="1px", data-lg-stick-up-offset="1px" data-stick-up='true' data-sm-stick-up='true' data-md-stick-up='true' data-lg-stick-up='true')
    .rd-navbar-inner
      .rd-navbar-aside-left
        .rd-navbar-nav-wrap
          include ../_centered-navigation

      // RD Navbar Panel
      .rd-navbar-panel
        // RD Navbar Toggle
        button.rd-navbar-toggle(data-rd-navbar-toggle='.rd-navbar-nav-wrap'): span

        // RD Navbar Brand
        .rd-navbar-brand
          a.brand-name(href='index.html')
            +png(logoDefault, 'logo-default')
            +png(logoInverse, 'logo-inverse')

      .rd-navbar-collapse-toggle(data-rd-navbar-toggle='.rd-navbar-collapse'): span
      .rd-navbar-aside-right.rd-navbar-collapse
        // RD Navbar Search
        .rd-navbar-search
          a.rd-navbar-search-toggle(data-rd-navbar-toggle=".rd-navbar-search" href="#")
            span
          form.rd-search(action="search-results.html" data-search-live="rd-search-results-live" method="GET")
            div.form-wrap
              label.form-label(for="rd-navbar-search-form-input" class="form-label") Search...
              input(class="rd-navbar-search-form-input form-input" id="rd-navbar-search-form-input" type="text" name="s" autocomplete="off")
              .rd-search-results-live
            button.rd-search-form-submit.mdi.mdi-magnify
        a(href='services.html').button.button-sm.button-default-outline book your service